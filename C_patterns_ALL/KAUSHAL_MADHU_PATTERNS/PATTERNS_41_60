PATTERN 47
* * * * * 
  *       *       
    *       *     
      *       *   
        * * * * * 

#include <stdio.h>

int main()
{
    int i,j;
    
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=9;j++)
        {
            if(i==1)
            {
                if(j<=5)
                {
                    printf("* ");
                }
            }
            
            if((i>=2)&&(i<=4))
            {
                if(j==i || j==(4+i))
                {
                    printf("* ");
                }
                else
                {
                    printf("  ");
                }
            }
            
            if(i==5)
            {
                if(j>=5)
                {
                    printf("* ");
                }
                else
                {
                    printf("  ");
                }
                
            }
            
        }
        printf("\n");
    }
}
