PATTERN 49
* * * * *         
  * * * * *       
    * * * * *     
      * * * * *   
        * * * * * 
      * * * * *   
    * * * * *     
  * * * * *       
* * * * * 

#include <stdio.h>

int main()
{
    int i,j;
    
    for(i=1;i<=9;i++)
    {
        for(j=1;j<=9;j++)
        {
            if(i<=5)
            {
                if(j>=i && j<=(4+i))
                {
                    printf("* ");
                }
            else
                {
                    printf("  ");
                }
            }
            
            if(i>=6)
            {
                if(j>=(10-i) && j<=(14-i))
                {
                    printf("* ");
                }
            else
                {
                    printf("  ");
                }
            }
            
            
        }
        printf("\n");
    }
}

PATTERN 49 PART B

        * * * * * 
      * * * * *   
    * * * * *     
  * * * * *       
* * * * *         
      * * * * *   
    * * * * *     
  * * * * *       
* * * * *

#include <stdio.h>

int main()
{
    int i,j;
    
    for(i=1;i<=9;i++)
    {
        for(j=1;j<=9;j++)
        {
            if(i<=5)
            {
                if(j>=(6-i) && j<=(10-i))
                {
                    printf("* ");
                }
            else
                {
                    printf("  ");
                }
            }
            
            if(i>=6)
            {
                if(j>=(10-i) && j<=(14-i))
                {
                    printf("* ");
                }
            else
                {
                    printf("  ");
                }
            }
            
            
        }
        printf("\n");
    }
}

************************************
PATTERN 50
        * * * * * 
      * * * * *   
    * * * * *     
  * * * * *       
* * * * *         
  * * * * *       
    * * * * *     
      * * * * *   
        * * * * * 

#include <stdio.h>

int main()
{
    int i,j;
    
    for(i=1;i<=9;i++)
    {
        for(j=1;j<=9;j++)
        {
            if(i<=5)
            {
                if(j>=(6-i) && j<=(10-i))
                {
                    printf("* ");
                }
            else
                {
                    printf("  ");
                }
            }
            
            if(i>=6)
            {
                if(j>=(i-4) && j<=i)
                {
                    printf("* ");
                }
            else
                {
                    printf("  ");
                }
            }
            
            
        }
        printf("\n");
    }
}

