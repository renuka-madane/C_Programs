PATTERN 45

*        
 **      
  ***    
   ****  
    *****

#include <stdio.h>

int main()
{
    int i,j;
    
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=9;j++)
        {
            if(j>=i && (j<=(2*i-1)))
            {
                
                printf("*");
            }
            else
            {
                printf(" ");
            }
        }
        printf("\n");
    }
}
