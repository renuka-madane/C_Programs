PATTERN 51
* * * * * * * * * 
* *           * * 
*   *       *   * 
*     *   *     * 
*       *       * 
*     *   *     * 
*   *       *   * 
* *           * * 
* * * * * * * * * 

#include <stdio.h>

int main()
{
    int i,j;
    
    for(i=1;i<=9;i++)
    {
        for(j=1;j<=9;j++)
        {
            if(i==1 || j==1 || i==9 || j==9 || (i==j) || (j==(10-i)))
            {
                printf("* ");
            }
            else
            {
                printf("  ");
            }
            
            
        }
        printf("\n");
    }
}
