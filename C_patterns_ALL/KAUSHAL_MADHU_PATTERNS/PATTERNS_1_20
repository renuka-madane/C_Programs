#include <stdio.h>

int main()
{
    int i,j,rows,col;
    
    
    for(i=1;i<=9;i=i+2)
    {
        for(j=1;j<=i;j=j+2)
        {
            printf("%d",j); 
        }
        printf("\n");
    }
}
  ORR

int main()
{
    int i,j,rows,col;
    
    
    for(i=1;i<=9;i+=2)
    {
        for(j=1;j<=i;j++)
        {
            if(j%2!=0)
            {
                printf("%d",j);
            }
            
        }
        printf("\n");
    }
}
