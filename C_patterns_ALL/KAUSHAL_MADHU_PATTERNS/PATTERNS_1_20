1==Enter no. of rows.5
Enter no. of columns.5
*
**
***
****
*****

#include <stdio.h>

int main()
{
    int i,j,rows,col;
    printf("Enter no. of rows.");
    scanf("%d",&rows);
    printf("Enter no. of columns.");
    scanf("%d",&col);
    
    for(i=1;i<=rows;i++)
    {
        for(j=1;j<=i;j++)
        {
            printf("*");
        }
        printf("\n");
    }
    
}

