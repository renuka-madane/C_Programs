#include <stdio.h>

int main()
{
    int i,j,k=0;
    
    
    for(i=1;i<=5;i++)
    {
        for(j=i;j<=5;j++)
        {
            
              printf("%c ",96+i);  
            
        }
        printf("\n");
    }
}
