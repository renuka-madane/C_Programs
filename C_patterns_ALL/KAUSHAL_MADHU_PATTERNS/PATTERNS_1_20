#include <stdio.h>

int main()
{
    int i,j;
    
    for(i=1;i<=5;i++)
    {
        for(j=4;j>=i;j--)
        {
            printf(" ");
        }
        
        for(j=i;j>=1;j--)
        {
            printf("%d",j%2);
        }
        
        printf("\n");
    }
}

ORR

int main()
{
    int i,j;
    
    for(i=1;i<=5;i++)
    {
        
        for(j=1;j<=5;j++)
        {
            if(j>=(6-i))
            {
                printf("%d",6-j);
            }
            else
            {
                printf(" ");
            }
        }
        
        printf("\n");
    }
}
