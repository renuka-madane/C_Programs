PATTERN 23
  E D C B A E D C B 
    D C B A D C B 
      C B A C B 
        B A B 
          A 

int main()
{
    int i,j;
    
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=i;j++)
        {
            printf("  ");
        }
        
        for(j=i;j<=5;j++)
        {
            printf("%c ",70-j);
        }
        
        for(j=i;j<=4;j++)
        {
            printf("%c ",70-j);
        }
        printf("\n");
        
    }
}

