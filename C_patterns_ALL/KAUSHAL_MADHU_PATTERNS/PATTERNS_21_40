PATTERN 27
*     *
 *   * 
  * *  
   *   
  * *  
 *   * 
*     *

#include <stdio.h>

int main()
{
    int i,j,n;
    
    for(i=1;i<=7;i++)
    {
        for(j=1;j<=7;j++)
        {
            if((i==j) || j==(8-i))
            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
                
        }
        
        printf("\n");
    }     
    
}
