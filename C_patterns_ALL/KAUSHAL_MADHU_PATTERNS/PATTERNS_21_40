#include <stdio.h>

int main()
{
    int i,j;
    
    for(i=1;i<=9;i++)
    {
        if(i<=5)
        {
            for(j=1;j<=5;j++)
        {
            
            if(j>=i)
            {
                printf("* ");
            }
            else
            {
                printf("  ");
            }
            
        }
            printf("\n");
        }
        
        if(i>5)
        {
            for(j=1;j<=5;j++)
            {
                if(j>=(10-i))
                {
                printf("* ");
            }
            else
            {
                printf("  ");
            }
            }
            printf("\n");
        }
        
    }
        
}
