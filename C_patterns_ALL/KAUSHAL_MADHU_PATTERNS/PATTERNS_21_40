int main()
{
    int i,j;
    
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=9;j++)
        {
            if((j<=i) || (j>=(10-i)))
            {
                
                printf("* ");
            }
            else{
                printf("  ");
            }
                
        }
        
        
        printf("\n");
    } 
    
    for(i=1;i<=4;i++)
    {
        for(j=4;j>=1;j--)
        {
            if(j>=i)
            {
                printf("* ");
                
            }
            else
            {
                printf("  ");
            }
            
        }
        
        for(j=1;j<=i;j++)
        {
            printf("  ");
        }
        for(j=4;j>=i;j--)
        {
            printf("* ");
        }
        
        printf("\n");
        
    }
