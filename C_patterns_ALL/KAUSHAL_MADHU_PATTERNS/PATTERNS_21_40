#include <stdio.h>

int main()
{
    int i,j;
    
    for(i=5;i>=1;i--)
    {
        for(j=1;j<=5;j++)
        {
            if(j<=i-1)
            {
                printf(" ");
            }
            else
            {
                printf("*");
            }
        }
        printf("\n");
    }
    
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=5;j++)
        {
            if(j<=i)
            {
                printf(" ");
            }
            else
            {
                printf("*");
            }
        }
        printf("\n");
    }
}


