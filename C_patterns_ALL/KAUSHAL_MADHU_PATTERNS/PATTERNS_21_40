#include <stdio.h>

int main()
{
    int i,j;
    
    for(i=1;i<=9;i++)
    {
        if(i<=5)
        {
            for(j=1;j<=9;j++)
        {
            if((j==(6-i)||(j==(4+i))))
            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
            
        }
        printf("\n");
        }
        
        if(i>5)
        {
            for(j=1;j<=9;j++)
        {
            if((j==(i-4)||(j==(14-i))))
            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
            
        }
        printf("\n");
        }
        
        
    }
}
