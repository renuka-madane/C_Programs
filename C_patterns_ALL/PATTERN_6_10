PATTERN 7
 Enter no. of rows.5
 Enter no. of columns.5
* * * * *
* * * * *
* * * * *
* * * * *
* * * * *

#include<stdio.h>
#include<stdlib.h>

int main()
{
	int rows,col;
	printf(" Enter no. of rows.");
	scanf("%d",&rows);
	printf(" Enter no. of columns.");
	scanf("%d",&col);
	
	int i,j;
	for(i=1;i<=rows;i++)
	{
		for(j=1;j<=col;j++)
	{
		printf("* ");
	}
	printf("\n");
	}	
	
}
