PATTERN 3
Enter no. of rows.6
 Enter no. of columns.6
*
**
***
****
*****
******
#include<stdio.h>
#include<stdlib.h>

int main()
{
	int rows,col;
	printf(" Enter no. of rows.");
	scanf("%d",&rows);
	printf(" Enter no. of columns.");
	scanf("%d",&col);
	
	int i,j;
	for(i=1;i<=rows;i++)
	{
	for(j=1;j<=i;j++)
	{
		printf("*");
	}
	printf("\n");
	}	
	
}
************************************************
PATTERN 4
 Enter no. of rows.5
 Enter no. of columns.5
5
5 4
5 4 3
5 4 3 2
5 4 3 2 1


#include<stdio.h>
#include<stdlib.h>

int main()
{
	int rows,col;
	printf(" Enter no. of rows.");
	scanf("%d",&rows);
	printf(" Enter no. of columns.");
	scanf("%d",&col);
	
	int i,j;
	for(i=5;i>=1;i--)
	{
	for(j=5;j>=i;j--)
	{
		printf("%d ",j);
	}
	printf("\n");
	}	
	
}

