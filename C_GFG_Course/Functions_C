#include <stdio.h>
int main()
{
    int (*ptr)(int ) = fun; // ptr is a pointer to function fun
    (*ptr)(3); --fun called using pointer
    return 0;
}
int fun(int n)
{
  for(;n > 0; n--)
    printf("GeeksQuiz ");
  return 0;
}  compiler error, since fun is not declared / defined b4 its assinged to ptr
